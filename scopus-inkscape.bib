@ARTICLE{Alawneh2020,
	author = {Alawneh, John I. and Vezina, Ben and Ramay, Hena R. and Al-Harbi, Hulayyil and James, Ameh S. and Soust, Martin and Moore, Robert J. and Olchowy, Timothy W. J.},
	title = {Survey and Sequence Characterization of Bovine Mastitis-Associated Escherichia coli in Dairy Herds},
