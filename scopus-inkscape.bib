@ARTICLE{Barbero20171427,
	author = {Barbero, Marco and Fernández-de-las-Peñas, César and Palacios-Ceña, María and Cescon, Corrado and Falla, Deborah},
	title = {Pain extent is associated with pain intensity but not with widespread pressure or thermal pain sensitivity in women with fibromyalgia syndrome},
